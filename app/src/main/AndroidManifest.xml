<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="purduecom.sam.mind_app">

    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".GameActivity"
            android:label="@string/title_activity_game"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Calibrate_Activity"
            android:label="@string/title_activity_calibrate_"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".exercise_Activity"
            android:label="@string/title_activity_exercise_"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".blue"
            android:label="@string/title_activity_blue"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".SP02"
            android:label="@string/title_activity_sp02"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>